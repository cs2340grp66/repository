<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.weichen.water_report.controllers.Historical_report_Activity">

    <ImageButton
        android:id="@+id/goBack_from_historical "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="?attr/actionModeCloseDrawable"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="ContentDescription,MissingConstraints"
        android:contentDescription=""
        android:onClick="goBack_from_historical"/>

    <ListView
        android:id="@+id/historical_report_list"
        android:layout_width="398dp"
        android:layout_height="441dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="128dp"
        android:layout_below="@+id/goBack_from_historical "
        android:layout_alignParentStart="true" />
</RelativeLayout>
